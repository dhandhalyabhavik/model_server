input_stream: "PYOBJECT:in"
output_stream: "PYOBJECT:out"

node {
  name: "preprocessor"
  calculator: "PythonExecutorCalculator"
  input_side_packet: "PYSESSION:pysession"
  input_stream: "PYOBJECT:in"
  output_stream: "PYOBJECT:preprocessed"
  node_options: {
    [type.googleapis.com / mediapipe.PythonExecutorCalculatorOptions]: {
      handler_path: "/ovms/preprocessor.py"
    }
  }
}
node {
  calculator: "PythonObjectToOVTensorConverterCalculator"
  input_stream: "PYOBJECT:preprocessed"
  output_stream: "OVTensor:preprocessed_ov"
}
node {
  calculator: "ModelAPISessionCalculator"
  output_side_packet: "SESSION:session"
  node_options: {
    [type.googleapis.com / mediapipe.ModelAPIOVMSSessionCalculatorOptions]: {
      servable_name: "add"
      servable_version: "1"
    }
  }
}
node {
  calculator: "ModelAPISideFeedCalculator"
  input_side_packet: "SESSION:session"
  input_stream: "OVTensor:preprocessed_ov"
  output_stream: "OVTensor:result_ov"
}
node {
  calculator: "OVTensorConverterToPythonObjectCalculator"
  input_stream: "OVTensor:result_ov"
  output_stream: "PYOBJECT:result"
}
node {
  name: "postprocessor"
  calculator: "PythonExecutorCalculator"
  input_side_packet: "PYSESSION:pysession"
  input_stream: "PYOBJECT:result"
  output_stream: "PYOBJECT:out"
  node_options: {
    [type.googleapis.com / mediapipe.PythonExecutorCalculatorOptions]: {
      handler_path: "/ovms/postprocessor.py"
    }
  }
}
